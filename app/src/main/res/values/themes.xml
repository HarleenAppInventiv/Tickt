<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Ticktapp" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="colorPrimary">@color/colorPrimaryDark</item>
        <item name="colorPrimaryDark">@color/white</item>
        <item name="colorAccent">@color/colorPrimaryDark</item>
        <item name="android:statusBarColor" tools:targetApi="l">@color/colorPrimaryDark</item>
        <item name="android:windowDisablePreview">true</item>
    </style>

    <style name="Theme.Ticktapp.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.Ticktapp.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.Ticktapp.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>
